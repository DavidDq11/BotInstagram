# Puerto donde correrá el bot de Instagram
PORT=3000

# Token de verificación del webhook que configuraste en Meta Developers
VERIFY_TOKEN=tu_verify_token

# Access token de la API de Instagram, obtenido desde Meta Developers
ACCESS_TOKEN=tu_instagram_access_token

# App Secret de tu aplicación de Meta Developers (si lo necesitas)
APP_SECRET=tu_instagram_app_secret

# ID de tu página de Facebook o cuenta de Instagram vinculada
PAGE_ID=tu_facebook_page_id

# URL de redirección para el OAuth (si usas OAuth en tu app)
REDIRECT_URI=tu_redirect_uri
